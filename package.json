{
  "name": "myblindtest",
  "version": "0.0.0",
  "engines": {
    "node": "12.x",
    "npm": "6.x"
  },
  "scripts": {
    "build": "npm run build:angular && npm run log && npm run build:node",
    "log": "dir",
    "build:angular": "cd ./src/front && npm install && npm run build:prod",
    "build:node": "node ./util/build.js && npm run transpile",
    "tsc": "tsc --build tsconfig.prod.json",
    "transpile": "npm run tsc",
    "lint": "tslint --project \"tsconfig.json\"",
    "start": "node -r ./dist",
    "start:dev": "nodemon --config nodemon.json",
    "start:both": "concurrently --kill-others-on-fail \"npm run start:angular\" \"nodemon --config nodemon.json \"",
    "start:angular": "cd ./src/front && ng serve -o",
    "test": "nodemon --config nodemon.test.json",
    "debug": "nodemon --config nodemon.json"
  },
  "_moduleAliases": {},
  "dependencies": {
    "@types/cors": "^2.8.8",
    "child-process": "^1.0.2",
    "command-line-args": "^5.1.1",
    "cookie-parser": "^1.4.5",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "elasticlunr": "^0.9.5",
    "ent": "^2.2.0",
    "es6-request": "^2.3.7",
    "express": "^4.17.3",
    "express-async-errors": "^3.1.1",
    "fs-extra": "^9.0.1",
    "helmet": "^3.22.0",
    "http-status-codes": "^1.4.0",
    "module-alias": "^2.2.2",
    "morgan": "^1.10.0",
    "querystring": "~0.2.0",
    "socket.io": "^2.3.0",
    "tslib": "^2.0.0",
    "winston": "^3.2.1"
  },
  "devDependencies": {
    "@types/command-line-args": "^5.0.0",
    "@types/cookie-parser": "^1.4.2",
    "@types/express": "^4.17.6",
    "@types/find": "^0.2.1",
    "@types/helmet": "0.0.46",
    "@types/jasmine": "^3.5.10",
    "@types/jsonfile": "^5.0.0",
    "@types/morgan": "^1.9.0",
    "@types/node": "^13.13.5",
    "@types/supertest": "^2.0.9",
    "find": "^0.3.0",
    "jasmine": "^3.5.0",
    "jsonfile": "^6.0.1",
    "nodemon": "^2.0.3",
    "supertest": "^4.0.2",
    "ts-node": "^8.10.1",
    "tsconfig-paths": "^3.9.0",
    "tslint": "^6.1.2",
    "typescript": "^3.8.3"
  },
  "prettier": {
    "singleQuote": true
  }
}
